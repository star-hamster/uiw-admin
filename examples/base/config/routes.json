[
  {
    "path": "/login",
    "element": "@/layouts/UserLayout"
  },
  {
    "path": "/",
    "element": "@/layouts/BasicLayout",
    "children": [
      {
        "index": true,
        "redirect": "/tableList"
      },
      {
        "path": "/home",
        "name": "首页",
        "element": "@/pages/TableList",
        "icon": "home"
      },
      {
        "path": "/tableList",
        "name": "查询表格",
        "element": "@/pages/TableList",
        "icon": "search"
      },
      {
        "path": "/form",
        "name": "数据录入",
        "element": "@/pages/Form",
        "icon": "search"
      },
      {
        "path": "/:group/*",
        "name": "测试点击页面",
        "hideInMenu": true,
        "component": "@/pages/TableList",
        "icon": "home"
      },
      {
        "path": "/dom",
        "name": "子项",
        "icon": "copy",
        "isAuth": true,
        "side": false,
        "children": [
          {
            "index": true,
            "redirect": "/dom/dashboard",
            "element": "@/pages/Dashboard"
          },
          {
            "path": "/dom/dashboard",
            "name": "Dashboard",
            "isAuth": true,
            "element": "@/pages/Dashboard"
          },
          {
            "path": "/dom/demo",
            "name": "Dashboard2",
            "element": "@/pages/Demo",
            "icon": "home"
          }
        ]
      },
      {
        "path": "/dom/exceptions",
        "name": "异常",
        "icon": "warning-o",
        "side": false,
        "children": [
          {
            "index": true,
            "redirect": "/dom/exceptions/403"
          },
          {
            "path": "/dom/exceptions/403",
            "name": "403",
            "element": "@uiw-admin/exceptions/esm/Exceptions/403"
          },
          {
            "path": "/dom/exceptions/500",
            "name": "500",
            "element": "@uiw-admin/exceptions/esm/Exceptions/500"
          },
          {
            "path": "/dom/exceptions/404",
            "name": "404",
            "element": "@uiw-admin/exceptions/esm/Exceptions/404"
          }
        ]
      },
      {
        "path": "/demo",
        "name": "列表查询/新增demo",
        "element": "@/pages/Demo",
        "icon": "home"
      }
    ]
  }
]
